{
	"name": "dem",
	"displayName": "dem",
	"description": "效率工具",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:dem.menuView",
		"onCommand:dem.version",
		"onCommand:dem.menuRevel",
		"onCommand:dem.menuChangeTitle",
		"onCommand:dem.webviewPanel.postForm",
		"onCommand:dem.helloWorld"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "dem.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "dem.version",
				"title": "Dem Version"
			},
			{
				"command": "dem.menuRevel",
				"title": "Dem Menu Revel",
				"icon": "public/d.svg"
			},
			{
				"command": "dem.menuChangeTitle",
				"title": "Dem ChangeTitle"
			},
			{
				"command": "dem.webviewPanel.postForm",
				"title": "Dem Start New PostForm Tool",
				"category": "Dem Tools"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dem",
					"title": "Dem",
					"icon": "public/d.svg"
				}
			]
		},
		"views": {
			"dem": [
				{
					"id": "dem.menuView",
					"name": "Dem",
					"icon": "public/d.svg"
				},
				{
					"id": "dem.menuRevel",
					"name": "revel",
					"contextualTitle": "123"
				},
				{
					"id": "dem.menuChangeTitle",
					"name": "changeTitle",
					"contextualTitle": "456"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "dem.menuRevel",
					"when": "view == dem.menuRevel",
					"group": "navigation"
				},
				{
					"command": "dem.menuChangeTitle",
					"when": "view == dem.menuChangeTitle",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "dem.menuRevel",
					"when": "view == dem.menuRevel && viewItem== dependency",
					"group": "inline"
				},
				{
					"command": "dem.menuChangeTitle",
					"when": "view == dem.menuChangeTitle && viewItem== dependency",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"ts-loader": "^9.2.8",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"@vscode/test-electron": "^2.1.3"
	}
}